using System;

namespace Fourth
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Enter a number: ");
            int num = int.Parse(Console.ReadLine());

            if (num % 6 == 0 && num % 7 == 0)
            {
                Console.WriteLine("Дели се и на двете");
            }else if (num % 6 == 0)
            {
                Console.WriteLine("Дели се само на 6");
            }else if (num % 7 == 0)
            {
                Console.WriteLine("Дели се само на 7");
            }else
            {
                Console.WriteLine("Числото не се дели нито на 6 нито на 7");
            }
        }
    }
}
